{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "STRIPE_SECRET": "@stripe_secret",
    "FRONTEND_URL": "@frontend_url"
  }
}
